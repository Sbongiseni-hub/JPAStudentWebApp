<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddWorkerServlet</servlet-name>
        <servlet-class>za.ac.tut.web.AddWorkerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetTotalServlet</servlet-name>
        <servlet-class>za.ac.tut.web.GetTotalServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUnder18Servlet</servlet-name>
        <servlet-class>.GetUnder18Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Below18</servlet-name>
        <servlet-class>za.ac.tut.web.Below18</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddWorkerServlet</servlet-name>
        <url-pattern>/AddWorkerServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetTotalServlet</servlet-name>
        <url-pattern>/GetTotalServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUnder18Servlet</servlet-name>
        <url-pattern>/GetUnder18Servlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Below18</servlet-name>
        <url-pattern>/Below18.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
